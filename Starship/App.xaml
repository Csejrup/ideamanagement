<Application 
            x:Class="Starship.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Starship"
             xmlns:viewmodels="clr-namespace:Starship.ViewModel"
             xmlns:views="clr-namespace:Starship.View.Pages">

    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewmodels:AddCustomerViewModel}">
                <views:AddCustomer/>
            </DataTemplate>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
