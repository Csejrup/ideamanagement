<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="asserts\1.png" />
    <None Remove="asserts\2.png" />
    <None Remove="asserts\3.jpg" />
    <None Remove="asserts\4.jpg" />
    <None Remove="asserts\dropbox.png" />
    <None Remove="asserts\googledrive.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MvvmCross" Version="7.1.2" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="asserts\1.png" />
    <Resource Include="asserts\2.png" />
    <Resource Include="asserts\3.jpg" />
    <Resource Include="asserts\4.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="asserts\dropbox.png" />
    <Resource Include="asserts\googledrive.png" />
  </ItemGroup>

</Project>